<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dp_100"/>
            <solid android:color="@color/translucent_white_50"/>
            <size android:height="50dp" android:width="100dp"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <shape>
            <size android:height="50dp" android:width="100dp"/>
            <corners android:radius="@dimen/dp_100"/>
            <solid android:color="@color/transparent"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <size android:height="50dp" android:width="100dp"/>
                <corners android:radius="@dimen/dp_100"/>
                <gradient android:startColor="@color/main_green" android:endColor="@color/blue_665b" android:angle="-45"/>
            </shape>
        </clip>
    </item>
</layer-list>
