<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dp_10"/>
            <size android:height="@dimen/dp_80" android:width="@dimen/dp_120"/>
            <gradient android:startColor="@color/blue_21a3fd_50" android:endColor="@color/blue_9085_50" android:angle="-45"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp_10"/>
                <size android:height="@dimen/dp_80" android:width="@dimen/dp_120"/>
                <solid android:color="@color/translucent_black_70"/>
            </shape>
        </clip>
    </item>
</layer-list>
